{
    "name": "kook-client",
    "version": "1.0.3",
    "description": "nodejs 平台 kook机器人开发SDK",
    "type": "module",
    "main": "lib/index.js",
    "engines": {
        "node": ">=16"
    },
    "scripts": {
        "start": "node .",
        "compile": "tsc --project tsconfig.build.json && tsc-alias -p tsconfig.build.json",
        "dtsc": "dtsc --project tsconfig.dtsc.json",
        "clean": "rm -rf ./lib",
        "build": "pnpm run compile && pnpm run dtsc",
        "dev": "tsx --tsconfig tsconfig.json ./src/test.ts",
        "pub": "pnpm publish --access public",
        "docs:dev": "vitepress dev docs --port 5566 --host 0.0.0.0",
        "docs:build": "vitepress build docs",
        "docs:serve": "vitepress serve docs",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "files": [
        "lib",
        "LICENSE",
        "README.md"
    ],
    "repository": {
        "url": "https://github.com/zhinjs/kook-client.git",
        "type": "git"
    },
    "keywords": [
        "kook",
        "robot",
        "开黑啦"
    ],
    "author": "凉菜",
    "license": "MIT",
    "dependencies": {
        "axios": "^1.7.9",
        "dotenv": "^17.2.1",
        "formdata-node": "^6.0.3",
        "log4js": "^6.9.1",
        "node-persist": "^4.0.4",
        "reconnecting-websocket": "^4.4.0",
        "ws": "^8.18.2"
    },
    "devDependencies": {
        "@types/node": "^24.3.0",
        "@types/ws": "^8.5.10",
        "dtsc": "^2.3.0",
        "tsc-alias": "^1.8.16",
        "tsconfig-paths": "^4.2.0",
        "tsx": "^4.19.2",
        "typescript": "^5.9.2",
        "vitepress": "^1.6.4"
    },
    "pnpm": {
        "overrides": {
            "markdown-it@<12.3.2": ">=12.3.2",
            "nth-check@<2.0.1": ">=2.0.1",
            "vite@<2.9.16": ">=2.9.16",
            "vite@<3.2.11": ">=3.2.11",
            "vite@<=3.2.10": ">=3.2.11",
            "vite@<2.9.13": ">=2.9.13",
            "vite@<=4.5.5": ">=4.5.6",
            "vite@<4.5.10": ">=4.5.10",
            "vite@<4.5.11": ">=4.5.11",
            "vite@<4.5.13": ">=4.5.13",
            "vite@<=4.5.13": ">=4.5.14",
            "vite@<4.5.12": ">=4.5.12",
            "vite@<=5.4.19": ">=5.4.20",
            "postcss@<8.4.31": ">=8.4.31"
        }
    }
}
